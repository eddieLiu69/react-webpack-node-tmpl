{
  "name": "react-webpack-node",
  "version": "1.0.0",
  "description": "A simple Node.js app using Express 4 with Webpack, React, alt, ImmutableJS",
  "repository": "https://github.com/eddieLiu69/react-webpack-node-tmpl.git",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint app",
    "clean": "rimraf public",
    "start": "NODE_ENV=production node build/index.js",
    "dev": "npm run watch & NODE_ENV=development nodemon",
    "build:dev": "NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "NODE_ENV=production webpack --colors --config ./webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:prod",
    "watch": "tcm . -p app/scss/**/*.scss -w & tsc",
    "test": "NODE_ENV=test karma start",
    "test:watch": "NODE_ENV=test npm test -- --watch --no-single-run",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "5.x"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "body-parser": "^1.14.2",
    "deep-equal": "^1.0.1",
    "express": "^4.13.3",
    "lodash": "^4.11.1",
    "method-override": "^2.3.5",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.2",
    "react-router-redux": "^4.0.0",
    "react-transform-hmr": "^1.0.2",
    "redux": "^3.0.5",
    "redux-logger": "^2.0.3",
    "redux-thunk": "^2.0.1",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "devDependencies": {
    "css-loader": "^0.23.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-react": "^4.1.0",
    "expect": "^1.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "inline-environment-variables-webpack-plugin": "0.0.2",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-jsdom-launcher": "^3.0.0",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "nock": "^8.0.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.8.1",
    "react-addons-test-utils": "^15.0.1",
    "react-stateless-wrapper": "^1.0.5",
    "react-transform-catch-errors": "^1.0.2",
    "redux-devtools": "^3.2.0",
    "redux-mock-store": "^1.0.2",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tsd": "^0.6.5",
    "typed-css-modules": "^0.1.6",
    "typescript": "^1.8.10",
    "url-loader": "^0.5.7"
  }
}
